{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "hytale-russia",
  "version": "0.1.0",
  "identifier": "com.aer.hytale-russia",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Hytale RU",
        "width": 550,
        "minWidth": 550,
        "maxWidth": 550,
        "height": 650,
        "minHeight": 650,
        "maxHeight": 650,
        "fullscreen": false,
        "resizable": false,
        "maximizable": false,
        "minimizable": true,
        "closable": true,
        "decorations": true,
        "alwaysOnTop": false,
        "skipTaskbar": false,
        "center": true,
        "visible": true,
        "transparent": false
      }
    ],
    "security": {
      "csp": null,
      "devCsp": null,
      "freezePrototype": true,
      "dangerousDisableAssetCspModification": false
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "assets/**/*"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "nsis": {
        "displayLanguageSelector": false,
        "languages": ["Russian"],
        "installerIcon": "icons/icon.ico",
        "installMode": "currentUser"
      },
      "allowDowngrades": true
    },
    "createUpdaterArtifacts": false,
    "externalBin": []
  }
}
